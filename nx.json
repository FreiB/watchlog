{
  "npmScope": "watchlog",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "collect-service": {
      "tags": ["nx-dotnet"]
    },
    "dotnet-data-layer": {
      "tags": ["nx-dotnet"]
    },
    "ngx-watchlog": {
      "tags": []
    },
    "shared": {
      "tags": []
    },
    "watchlog-dash": {
      "tags": []
    },
    "watchlogjs": {
      "tags": []
    }
  },
  "plugins": ["@nx-dotnet/core"]
}
